<script type="text/javascript">
                
    var form = document.querySelector('#formDownload');
    var button = form.querySelector('#submitDownload');

    async function submitDownload(frm, token) {
        HTMLFormElement.prototype.submit.call(frm);
        if (token) {
            try {
                await new Promise((resolve, reject) => {
                    form.querySelector('#token').value = token;
                    form.submit();
                    grecaptcha.reset();
                    resolve();
                });

                var textDownloading = document.querySelector('.text-downloading');
                if (textDownloading && textDownloading.parentNode) {
                    textDownloading.parentNode.removeChild(textDownloading);
                }

                button.classList.remove('button--loading-download');
                
            } catch (err) {
                console.error('Error in download with captcha', token, err);
            }
        }
    }

    var renderDownload = function() {
        var recaptcha = document.querySelector('.recaptcha-download');

        (function(frm) {
            var recaptchaId = grecaptcha.render(recaptcha, {
            'sitekey': '6LdZqaMZAAAAAMi-EaIh8xjypOnZsUDt0a9B468g',
            'size': 'invisible',
            'callback': function (token) {
                    submitDownload(frm, token);
                }
            });

            frm.onsubmit = function (event) {
                event.preventDefault();
                event.stopPropagation();

                button.classList.add('button--loading-download');

                var textDownloading = document.createElement('span');
                textDownloading.className = 'text-downloading';
                textDownloading.innerHTML = 'Downloading...';
                button.appendChild(textDownloading);

                grecaptcha.execute(recaptchaId);
            };
        })(form);
    };
</script>
