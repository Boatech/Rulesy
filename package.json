{
  "name": "phaxad",
  "version": "1.0.0",
  "description": "A development platform that helps developers build fast and easy projects.",
  "scripts": {
    "tailwind": "tailwindcss build public/assets/css/tailwind.css -o public/assets/css/tailwind.output.css",
    "build": "env NODE_ENV=production postcss public/assets/css/tailwind.css -o public/assets/css/tailwind.output.css",
    "cz": "git-cz",
    "release": "release-it"
  },
  "author": "Nnamchi Israel <nnamchiisrael@yahoo.com>",
  "license": "MIT",
  "devDependencies": {
    "@release-it/conventional-changelog": "1.1.4",
    "@tailwindcss/custom-forms": "0.2.1",
    "autoprefixer": "9.8.0",
    "color": "3.1.2",
    "commitizen": "^4.2.4",
    "cssnano": "4.1.10",
    "cz-conventional-changelog": "3.2.0",
    "postcss-cli": "7.1.1",
    "release-it": "13.6.4",
    "tailwindcss": "1.4.6",
    "tailwindcss-multi-theme": "1.0.3"
  },
  "keywords": [
    "development",
    "phaxad",
    "team collaboration",
    "advance",
    "system"
  ],
  "release-it": {
    "github": {
      "release": true
    },
    "npm": {
      "publish": false
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular",
        "infile": "CHANGELOG.md"
      }
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@themesberg/flowbite": "^1.2.0",
    "daisyui": "^1.21.0",
    "sharect": "^2.0.0",
    "vue": "^2.6.14",
    "vue-tailwind": "^2.5.0"
  }
}
